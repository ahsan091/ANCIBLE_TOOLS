# Wazuh Docker Compose Override
# Generated by Ansible - Do not edit manually
# Attaches all services to external soc_net network

services:
  wazuh.manager:
    networks:
      - default
      - {{ soc_network_name }}

  wazuh.indexer:
    ports:
      - "{{ wazuh_indexer_port }}:9200"
    networks:
      - default
      - {{ soc_network_name }}

  wazuh.dashboard:
    ports:
      - "{{ wazuh_dashboard_port }}:5601"
    networks:
      - default
      - {{ soc_network_name }}

networks:
  default:
    driver: bridge
  {{ soc_network_name }}:
    external: true
