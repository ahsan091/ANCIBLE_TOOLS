# Shuffle Docker Compose Override
# Generated by Ansible - Do not edit manually
# Attaches all services to external soc_net network and configures ports

services:
  frontend:
    ports:
      - "{{ shuffle_frontend_http_port }}:80"
      - "{{ shuffle_frontend_https_port }}:443"
    networks:
      - default
      - {{ soc_network_name }}

  backend:
    ports:
      - "{{ shuffle_backend_port }}:5001"
    networks:
      - default
      - {{ soc_network_name }}

  orborus:
    networks:
      - default
      - {{ soc_network_name }}

  opensearch:
    ports:
      - "{{ shuffle_opensearch_port }}:9200"
    networks:
      - default
      - {{ soc_network_name }}

networks:
  default:
    driver: bridge
  {{ soc_network_name }}:
    external: true
